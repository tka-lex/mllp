    "url": "git@github.com:tka-lex/mllp.git"
  },
  "bugs": {
    "url": "https://github.com/tka-lex/mllp/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitest/coverage-istanbul": "^2.1.6",
    "eslint": "^9.16.0",
    "prettier": "^3.4.1",
